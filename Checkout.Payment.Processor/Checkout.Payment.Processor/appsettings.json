{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File" ],
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "Enrich": [ "FromLogContext" ],
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "{Timestamp:dd-MM-yyyy HH:mm:ss.fff} [{Level}] {Message:lj}{NewLine}"
        }
      },
      {
        "Name": "Debug",
        "Args": {
          "outputTemplate": "{Timestamp:dd-MM-yyyy HH:mm:ss.fff} [{Level}] {Message:lj}{NewLine}"
        }
      },
      {
        "Name": "File",
        "Args": {
          "path": ".\\Logs\\Checkout.Payment.Processor-.txt",
          "rollingInterval": "Day",
          "shared": true,
          "outputTemplate": "{Timestamp:dd-MM-yyyy HH:mm:ss.fff} [{Level}] {Message:lj}{NewLine}"
        }
      }
    ],
    "Properties": {
      "Application": "Checkout.Payment.Processor"
    }
  },
  "AuthenticationSettings": {
  },
  "SNSSettings": {
    "IsEnabled": true,
    "PaymentProcessTopicArn": "arn:aws:sns:us-east-1:000000000000:payment-process",
    "Region": "us-east-1",
    "UseHttp": true,
    "ServiceURL": "http://localhost:34566",
    "ProxyHost": "localhost",
    "ProxyPort": 34566,
    "Credentials": {
      "AccessKey": "foo",
      "SecretKey": "bar"
    }
  },
  "MicroServiceSettings": {
    "PaymentCommandBaseAddress": "https://localhost:8002",
    "AcquiringBankBaseAddress": "https://localhost:8003",
    "AcquiringBankAuthorization": "checkout.payment.processor-apikey"
  }
}